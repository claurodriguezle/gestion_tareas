{
    "builds": [
      {
        "src": "gestion_tareas/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.12.6"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "gestion_tareas/wsgi.py"
      }
    ]
  }